{
  "name": "zeiw-desktop",
  "version": "1.0.0",
  "description": "Open source community-based game üèì",
  "repository": "git@github.com:next/zeiw-desktop.git",
  "license": "MIT",
  "author": "Casper <pnfcre@gmail.com>",
  "main": "app/main.js",
  "scripts": {
    "dist": "sh ./scripts/make-build-env.sh && electron-builder --win --dir -p never",
    "postinstall": "electron-builder install-app-deps",
    "start": "yarn install && sh ./scripts/make-build-env.sh && electron app/main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "devDependencies": {
    "electron": "^7.1.3",
    "electron-builder": "^22.2.0",
    "husky": "^3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "build": {
    "appId": "org.zeiw.zeiw"
  }
}
