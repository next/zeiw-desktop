{
  "name": "ZEIW",
  "version": "1.0.0",
  "repository": "git@github.com:ZEIW/desktop.git",
  "license": "MIT",
  "author": "ZEIW",
  "main": "app/main.js",
  "scripts": {
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "start": "yarn install && electron app/main.js"
  },
  "husky": {
